{
  "name": "browser-history-merger",
  "productName": "Browser History Merger",
  "version": "1.0.1",
  "description": "Cross platform, cross browser history merger.",
  "main": "loader.js",
  "scripts": {
    "start": "gulp",
    "test": "echo \"Error: no test specified\" && exit 1",
    "preinstall": "npm i sqlite3 --build-from-source",
    "postinstall": "sudo ./node_modules/.bin/electron-rebuild",
    "clean": "rm -rf releases",
    "pack": "npm run packer && npm run asar && npm run asar:clean",
    "packer": "npm run clean && electron-packager . --platform=darwin --arch=x64 --version=1.3.2 --ignore=src --ignore=assets --ignore=releases --ignore=README.md --ignore=launcher.icns --ignore=jsconfig.json --ignore=gulpfile.babel.js --icon=launcher.icns --build-version=1.3.5 --out=releases --overwrite=true --prune",
    "asar": "asar pack \"releases/Browser History Merger-darwin-x64/Browser History Merger.app/Contents/Resources/app\" \"releases/Browser History Merger-darwin-x64/Browser History Merger.app/Contents/Resources/app.asar\"",
    "asar:clean": "rm -rf \"releases/Browser History Merger-darwin-x64/Browser History Merger.app/Contents/Resources/app\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VenomVendor/Browser-History-Merger.git"
  },
  "keywords": [
    "Electron",
    "browser",
    "history",
    "merger",
    "cross platform",
    "cross browser"
  ],
  "author": "VenomVendor",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/VenomVendor/Browser-History-Merger/issues"
  },
  "homepage": "https://github.com/VenomVendor/Browser-History-Merger/#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-register": "^6.14.0",
    "better-console": "^0.2.4",
    "electron-packager": "^8.0.0",
    "del": "^2.2.2",
    "electron": "^1.3.5",
    "electron-prebuilt": "^1.3.5",
    "electron-rebuild": "^1.2.1",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-babel": "^6.1.2",
    "gulp-eslint": "^3.0.1",
    "gulp-js-obfuscator": "^1.0.0",
    "gulp-load-plugins": "^1.2.4",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7"
  },
  "dependencies": {
    "sqlite3": "^3.1.4"
  },
  "build": {
    "dir": "./",
    "name": "browser-history-merger",
    "platform": "darwin",
    "arch": "x64",
    "version": "1.3.2",
    "out": "./releases",
    "app-bundle-id": "",
    "app-version": "",
    "overwrite": true,
    "asar": true,
    "icon": "launcher.icns",
    "app_version": "1.3.5",
    "bundle_id": "",
    "appname": "Browser History Merger",
    "sourcedir": "./Browser-History-Merger",
    "ignore": "./releases"
  }
}
